digraph graph_name {
  graph [
    charset = "UTF-8";
    label = "example graph",
    labelloc = "t",
    labeljust = "c",
    bgcolor = "#343434",
    fontcolor = white,
    fontsize = 18,
    style = "filled",
    rankdir = LR,
    margin = 0.2,
    splines = spline,
    ranksep = 1.0,
    nodesep = 0.9
  ];

  node [
    colorscheme = "rdylgn11"
    style = "solid,filled",
    fontsize = 16,
    fontcolor = 6,
    fontname = "Migu 1M",
    color = 7,
    fillcolor = 11,
    fixedsize = true,
    height = 0.6,
    width = 1.2
  ];

  edge [
    style = solid,
    fontsize = 14,
    fontcolor = white,
    fontname = "Migu 1M",
    color = white,
    labelfloat = true,
    labeldistance = 2.5,
    labelangle = 70
  ];

  // node define
  alpha;
  beta [shape = box];
  gamma [shape = diamond];
  delta [shape = circle];


  // edge define
  alpha -> beta [label = "a-g"];
  alpha -> gamma [label = "a-b", arrowhead = normal];
  beta -> delta [label = "b-d", style = dotted];
  beta -> epsilon [label = "b-e", arrowhead = tee];
}